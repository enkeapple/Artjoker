{
	"name": "artjoker",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"start": "npx react-native start --reset-cache",
		"android": "npx react-native run-android",
		"ios": "npx react-native run-ios",
		"postinstall": "npx react-codemod rename-unsafe-lifecycles",
		"android:apk": "cd android && ./gradlew clean && ./gradlew assembleRelease && cd ../",
		"android:bundle": "cd android && ./gradlew clean && ./gradlew bundleRelease && cd ../",
		"android:clean": "cd android && ./gradlew clean && cd ../",
		"prettier": "prettier --write './src/**/*.{ts,tsx}'",
		"watchman": "watchman watch-del-all && rm -rf $TMPDIR/react-* && rm -rf node_modules/ && npm cache verify && npm install && npm start -- --reset-cache",
		"xcode": "xed -b ios"
	},
	"dependencies": {
		"@react-native-community/masked-view": "^0.1.6",
		"@react-navigation/native": "^5.0.1",
		"@react-navigation/native-stack": "^5.0.1",
		"@react-navigation/stack": "^5.0.1",
		"axios": "^0.19.2",
		"lodash": "^4.17.15",
		"mobx": "^5.15.4",
		"mobx-react": "6.1.7",
		"mobx-react-devtools": "^6.1.1",
		"moment": "^2.24.0",
		"react": "16.12.0",
		"react-native": "0.61.5",
		"react-native-gesture-handler": "^1.5.6",
		"react-native-material-dropdown": "^0.11.1",
		"react-native-material-ripple": "^0.9.1",
		"react-native-material-textfield": "^0.16.1",
		"react-native-reanimated": "^1.7.0",
		"react-native-safe-area-context": "^0.7.2",
		"react-native-screens": "^2.0.0-beta.2"
	},
	"devDependencies": {
		"@babel/core": "^7.8.4",
		"@babel/plugin-proposal-decorators": "^7.8.3",
		"@babel/runtime": "^7.8.4",
		"@types/lodash": "^4.14.149",
		"@types/react-native": "^0.61.12",
		"@types/react-native-material-dropdown": "^0.11.3",
		"@types/react-native-material-textfield": "^0.16.3",
		"@vovkasm/tslint-config": "^2.0.1",
		"babel-plugin-module-resolver": "^4.0.0",
		"metro-react-native-babel-preset": "^0.58.0",
		"prettier": "^1.19.1",
		"tslint": "^6.0.0",
		"tslint-plugin-prettier": "^2.1.0",
		"typescript": "^3.7.5"
	}
}
